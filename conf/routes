GET     /                             controllers.Application.index

GET     /playing                      controllers.Application.playing
GET     /playlist                     controllers.Application.playlist

GET     /login                        controllers.Application.login
GET     /callback                     controllers.Application.callback(code: Option[String], state: Option[String])
GET     /logout                       controllers.Application.logout

GET     /socket                       controllers.Application.socket

POST    /request/:id                  controllers.Application.request(id: String)

GET     /backend                      controllers.Application.backend

POST    /shift                        controllers.Application.shift
POST    /kick/:id                     controllers.Application.kick(id: String)

GET     /song/notFounds               controllers.Application.getNotFounds
GET     /song/key/:key                controllers.Application.getSongByKey(key: String)
GET     /song/id/:id                  controllers.Application.getSongsById(id: String)
POST    /setSongId                    controllers.Application.setSongId
POST    /mergeKeys                    controllers.Application.mergeKeys

GET     /assets/*file                 controllers.Assets.versioned(path="/public", file: Asset)
